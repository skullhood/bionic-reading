{
  "short_name": "Bionic Reading",
  "name": "Bionic Reading Unofficial.",
  "icons": 
    {
      "16": "logo16.png",
      "32": "logo32.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  ,
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo128.png",
    "default_title": "Settings"
  },
  "manifest_version": 3,
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["static/js/content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
